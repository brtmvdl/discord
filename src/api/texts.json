{
  "texts": [
    {
      "when": [{ "message.0.content": "Olá, gente!" }],
      "then": "Olá, meu nome é ${bots.name}"
    },
    {
      "when": [{ "message.0.content": "/clock" }],
      "then": "${clock.hours}:${clock.minutes}:${clock.seconds}"
    },
    {
      "when": [{ "message.0.content": "Quanto está o dolar?" }],
      "then": "Agora o dolar está em ${coins.USD.price}"
    }
  ]
}
